  "private": true,
  "description": "A React Router plugin to create router elegantly",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/elegant-router",
  "repository": {
