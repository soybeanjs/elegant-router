    "magicast": "0.3.5",
    "unplugin": "2.1.0"
  },
  "devDependencies": {
    "@elegant-router/core": "workspace:*",
    "@types/node": "22.10.2",
    "typescript": "5.7.2",
    "unbuild": "2.0.0"
