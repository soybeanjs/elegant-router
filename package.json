{
  "name": "elegant-router",
  "type": "module",
  "version": "1.0.2",
  "packageManager": "pnpm@10.12.1",
  "description": "A plugin to create router elegantly",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/elegant-router",
  "repository": {
    "url": "https://github.com/soybeanjs/elegant-router.git"
  },
  "bugs": {
    "url": "https://github.com/soybeanjs/elegant-router/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "bin": {
    "er": "dist/cli.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.mjs",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.mjs",
      "require": "./dist/esbuild.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.mjs",
      "require": "./dist/rspack.cjs"
    },
    "./rolldown": {
      "types": "./dist/rolldown.d.ts",
      "import": "./dist/rolldown.mjs",
      "require": "./dist/rolldown.cjs"
    },
    "./farm": {
      "types": "./dist/farm.d.ts",
      "import": "./dist/farm.mjs",
      "require": "./dist/farm.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": ["./dist/*", "./*"]
    }
  },
  "files": ["README.en_US.md", "dist", "README.md"],
  "scripts": {
    "build": "unbuild",
    "cleanup": "soy cleanup",
    "commit": "soy git-commit",
    "lint": "eslint . --fix",
    "publish-pkg": "pnpm publish --access public",
    "release": "pnpm soy release",
    "stub": "unbuild --stub",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "update-pkg": "soy ncu"
  },
  "dependencies": {
    "cac": "6.7.14",
    "chokidar": "4.0.3",
    "consola": "3.4.2",
    "enquirer": "2.4.1",
    "kolorist": "1.8.0",
    "picomatch": "4.0.2",
    "tinyglobby": "0.2.14",
    "ts-morph": "26.0.0",
    "unconfig": "7.3.2",
    "unplugin-utils": "0.2.4"
  },
  "devDependencies": {
    "@nuxt/kit": "3.17.5",
    "@nuxt/schema": "3.17.5",
    "@soybeanjs/cli": "1.3.1",
    "@soybeanjs/eslint-config": "1.6.1",
    "@types/node": "24.0.0",
    "@types/picomatch": "4.0.0",
    "@vitest/coverage-v8": "3.2.3",
    "@vitest/ui": "3.2.3",
    "eslint": "9.28.0",
    "eslint-plugin-vue": "10.2.0",
    "lint-staged": "16.1.0",
    "simple-git-hooks": "2.13.0",
    "tsx": "4.20.0",
    "typescript": "5.8.3",
    "unbuild": "3.5.0",
    "vitest": "3.2.3",
    "vue-eslint-parser": "10.1.3"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
